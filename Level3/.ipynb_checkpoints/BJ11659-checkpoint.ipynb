   "source": [
    "n,m = map(int, input().split())\n",
    "\n",
    "lst = list(map(int, input().split()))\n",
    "\n",
    "dp = [0]*(n+1)\n",
    "for i in range(1,n+1):\n",
    "    dp[i] = dp[i-1]+lst[i-1]\n",
    "print(dp)\n",
    "\n",
    "for _ in range(m):\n",
    "    a,b = map(int,input().split())\n",
    "    print(dp[b]-dp[a-1])"
   ]
