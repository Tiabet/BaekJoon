   "source": [
    "import math\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "dp=[0,1]\n",
    "\n",
    "for i in range(2,n+1):\n",
    "    tmpmax = 5\n",
    "    for j in range(1,int(math.sqrt(i))+1):\n",
    "        tmp = i - j**2\n",
    "        tmpmax = min(tmpmax,dp[tmp])\n",
    "        \n",
    "    dp.append(tmpmax+1)\n",
    "    \n",
    "print(dp[n])\n"
   ]
