   "source": [
    "n = int(input())\n",
    "space_up2 = 0\n",
    "space_up1 = 2\n",
    "for i in range(2,n+1):\n",
    "    if i % 2 == 0 :\n",
    "        space_up2 = space_up2 + 2*space_up1 + 2\n",
    "    else :\n",
    "        space_up1 = space_up1 + 2*space_up2 + 2\n",
    "if n%2 == 0 :\n",
    "    print((space_up2+1)%9901)\n",
    "else :\n",
    "    print((space_up1+1)%9901)"
   ]
  }
