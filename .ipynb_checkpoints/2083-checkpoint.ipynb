    "    name, age, weight = map(str, input().split())\n",
    "    if name == '#' :\n",
    "        break\n",
    "    age = int(age)\n",
    "    weight = int(weight)\n",
    "    if age > 17 or weight >= 80 :\n",
    "        print(name + ' Senior')\n",
    "    else : \n",
    "        print(name + ' Junior')"
