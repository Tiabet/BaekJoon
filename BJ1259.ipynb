   "source": [
    "팰린드롬수\n",
    "대칭인 수를 찾는 문제이다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "05b59039",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "12321\n",
      "yes\n",
      "1234\n",
      "no\n",
      "22\n",
      "yes\n",
      "2332\n",
      "yes\n",
      "0\n"
     ]
    }
   ],
   "source": [
    "while True :\n",
    "    num = input()\n",
    "    if num == '0':\n",
    "        break;\n",
    "    reverse_num = num[::-1]\n",
    "    if reverse_num == num:\n",
    "        print(\"yes\")\n",
    "    else:\n",
    "        print(\"no\")"
   ]
  },
  {
